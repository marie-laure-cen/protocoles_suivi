        "cd_nomenclature": ["TVX_AR","TVX_DEBR","TVX_DESO","TVX_ECO","TVX_EEE","TVX_GYR","TVX_ET","TVX_PL","TVX_TA"]
      },
      "hidden": "({value}) => value.obj_travaux != 'Lande/fourré/haie'",
      "multiple": true
    },
    "tvx_bois":{
      "type_widget": "datalist",
